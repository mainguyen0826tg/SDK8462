#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/nguyenmai/Gooddata/Bear/8.4.61/common/temp/node_modules/.pnpm/typescript@4.0.2/node_modules/typescript/bin/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/common/temp/node_modules/.pnpm/typescript@4.0.2/node_modules/typescript/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/common/temp/node_modules/.pnpm/typescript@4.0.2/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/common/temp/node_modules/.pnpm/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/common/temp/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/common/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/node_modules:/Users/nguyenmai/Gooddata/Bear/node_modules:/Users/nguyenmai/Gooddata/node_modules:/Users/nguyenmai/node_modules:/Users/node_modules:/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/examples/sdk-examples/node_modules/typescript/bin/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/examples/sdk-examples/node_modules/typescript/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/examples/sdk-examples/node_modules:/Users/nguyenmai/Gooddata/Bear/8.4.61/examples/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../typescript/bin/tsc" "$@"
fi
